<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mylist">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyList">
        <activity
            android:name=".admin.list.ListCategory"
            android:parentActivityName=".admin.form.FormCategory" />
        <activity
            android:name=".admin.list.ListProduct"
            android:parentActivityName=".admin.form.FormProduct" />
        <activity
            android:name=".admin.list.ListStore"
            android:parentActivityName=".admin.form.FormStore" />
        <activity
            android:name=".admin.list.ListShoppinglist"
            android:parentActivityName=".admin.form.FormShoppinglist" />
        <activity
            android:name=".admin.form.FormCategory"
            android:parentActivityName=".admin.HomeAdmin" />
        <activity
            android:name=".admin.form.FormProduct"
            android:parentActivityName=".admin.HomeAdmin" />
        <activity
            android:name=".admin.form.FormStore"
            android:parentActivityName=".admin.HomeAdmin" />
        <activity
            android:name=".admin.form.FormShoppinglist"
            android:parentActivityName=".admin.HomeAdmin" />
        <activity
            android:name=".admin.HomeAdmin"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".list.MyListsActivity"
            android:parentActivityName=".list.MyListActivity" />
        <activity
            android:name=".list.MyListActivityWs"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".list.MyListActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>